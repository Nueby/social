<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>EncodingFilter</filter-name>
  	<display-name>EncodingFilter</display-name>
  	<description></description>
  	<filter-class>filter.EncodingFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>EncodingFilter</filter-name>
  	<url-pattern>*.do</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>UserController</servlet-name>
  	<display-name>UserController</display-name>
  	<description></description>
  	<servlet-class>controller.UserController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SlipVerificationCode</servlet-name>
  	<display-name>SlipVerificationCode</display-name>
  	<description></description>
  	<servlet-class>controller.SlipVerificationCode</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SlipVerificationCodeController</servlet-name>
  	<display-name>SlipVerificationCodeController</display-name>
  	<description></description>
  	<servlet-class>controller.SlipVerificationCodeController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SendEmailController</servlet-name>
  	<display-name>SendEmailController</display-name>
  	<description></description>
  	<servlet-class>controller.SendEmailController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterController</servlet-name>
  	<display-name>RegisterController</display-name>
  	<description></description>
  	<servlet-class>controller.RegisterController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/UserController.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SlipVerificationCode</servlet-name>
  	<url-pattern>/SlipVerificationCode.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SlipVerificationCodeController</servlet-name>
  	<url-pattern>/SlipVerificationCodeController.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SendEmailController</servlet-name>
  	<url-pattern>/SendEmailController.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterController</servlet-name>
  	<url-pattern>/RegisterController.do</url-pattern>
  </servlet-mapping>
</web-app>
